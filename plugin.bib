  author = {Karamagi, Humphrey C and Muneene, Derrick and Droti, Benson and Jepchumba, Violet and Okeibunor, Joseph C and Nabyonga, Juliet and Asamani, James Avoka and Traore, Moussa and Kipruto, Hillary},

@article{sinaci2023dataa,
  title = {A {{Data Transformation Methodology}} to {{Create Findable}}, {{Accessible}}, {{Interoperable}}, and {{Reusable Health Data}}: {{Software Design}}, {{Development}}, and {{Evaluation Study}}},
